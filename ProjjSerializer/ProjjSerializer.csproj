<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <PackageId>ProjjSerializer</PackageId>
    <Authors>Projj</Authors>
    <Company>Projj</Company>
    <Description>A C# (.NET Core) binary serialization and network buffer encapsulation library designed with ease of use in mind.</Description>
    <PackageProjectUrl>https://github.com/projjm/ProjjSerializer</PackageProjectUrl>
    <PackageIcon>logo.png</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/projjm/ProjjSerializer.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Serializer, Deserializer, Binary Serialization, Buffer, Network</PackageTags>
    <PackageReleaseNotes>Initial nuget release.</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\Downloads\logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
